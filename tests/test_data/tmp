names = ['TSI avg', 'TSI max', 'TO avg', 'TO max', 'BM Score', 'Largest Match', 'SSU Min', 'SSU Avg', 'SSTU Min', 'SSTU Avg', 'Max Bad Nucleotide %', 'Max Defect Component', 'Mean Bad Nucleotide %', 'WSI-Intra', 'WSI-Inter', 'WSI-Intra-1', 'WSI-Inter-1', 'Verboten', 'WSI', 'TH energy avg', 'TH energy range']

iterate_list = zip(t.scores, t.true_val, t.fmts, names)
for score, tru, fmt, name in iterate_list:
    score_str = fmt.format(score)
    truth_str = fmt.format(tru)
    t.assertEqual(score_str, truth_str, 'Score {}'.format(name))




